{
  "manifest_version": 3,
  "name": "Veynn Downloader",
  "version": "3.1",
  "description": "Downloads all valid non-GIF images larger than 200 KB from the current manga, manhua, manhwa, comic, or donghua chapter page. May not work on some sites.",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab", "downloads", "scripting", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
